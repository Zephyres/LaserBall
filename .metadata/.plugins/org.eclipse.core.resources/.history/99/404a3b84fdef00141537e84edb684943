package entity;

import org.newdawn.slick.Image;
import org.newdawn.slick.SlickException;

public class LaserWave extends ProjectileEntity {

	public LaserWave(float s, float r, float d) throws SlickException {
		super(s, r, d);
		
		setImage(new Image("images/laserball.png").getScaledCopy(0.6f));
	}
	
}
